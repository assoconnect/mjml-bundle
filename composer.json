{
  "name": "assoconnect/mjml-bundle",
  "description": "MJML template support for Twig template engine",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["MJML", "Twig"],
  "authors": [
    {
      "name": "Assoconnect",
      "homepage": "https://www.assoconnect.com"
    },
    {
      "name": "Sylvain Fabre"
    }
  ],
  "autoload": {
    "psr-4": {
      "Assoconnect\\MJMLBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Assoconnect\\MJMLBundle\\Tests\\": "tests/",
      "Assoconnect\\MJMLBundle\\Tests\\Functional\\App\\": "tests/Functional/src/"
    }
  },
  "require": {
    "php": "^7.2",
    "symfony/framework-bundle": "^4.2|^5.0",
    "symfony/process": "^4.2|^5.0",
    "symfony/console": "^4.2|^5.0",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "symfony/yaml": "^4.2|^5.0"
  }
}
